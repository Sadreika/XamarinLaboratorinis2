<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Laboratorinis2.MainPage">

    <StackLayout Margin="20">
        <Button Text="1"
                x:Name="FirstButton"
                Clicked="FirstButton_Clicked"/>
        <Button Text="2"
                x:Name="SecondButton"
                Clicked="SecondButton_Clicked"/>
        <Button Text="3"
                x:Name="ThirdButton"
                Clicked="ThirdButton_Clicked"/>
        <Button Text="4"
                x:Name="FourthButton"
                Clicked="FourthButton_Clicked"/>
        <Entry Text="Čia bus išvedami sugeneruoti skaičiai nuo -100 iki 0"
               HorizontalOptions="Center"
               IsReadOnly="True"
               x:Name="NumberEntry"/>
        <Entry Text="Čia bus išvedami sugeneruoti skaičiai nuo 0 iki 100"
               HorizontalOptions="Center"
               IsReadOnly="True"
               x:Name="SecondNumberEntry"/>
       
    </StackLayout>
</ContentPage>
